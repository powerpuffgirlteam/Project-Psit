import matplotlib.pyplot as plt
plt.rcdefaults()
import numpy as np
import matplotlib.pyplot as plt

train = ('สายเหนือ', 'สายตะวันออกเฉียงเหนือ', 'สายตะวันออก', 'สายใต้')
y_pos = np.arange(len(train))
performance = 3 + 10 * np.random.rand(len(train))
error = np.random.rand(len(train))

plt.barh(y_pos, performance, xerr=error, align='center', alpha=0.4)
plt.yticks(y_pos, train)
plt.xlabel('Minute')
plt.title('The average delay of railway by region')

plt.show()
